@model HemsTask.Model.Models.ProductCategory

@{
    ViewData["Title"] = "details";
}

<h1>Details</h1>

<div>
	<h4>Product category</h4>

	<hr />

	<dl class="row">
		<dt class="col-sm-2">
			Category code
		</dt>
		<dd class="col-sm-10">
			@Html.DisplayFor(model => model.ProductCategoryCode)
		</dd>
		<dt class="col-sm-2">
			Category name
		</dt>
		<dd class="col-sm-10">
			@Html.DisplayFor(model => model.CategoryName)
		</dd>
		<dt class="col-sm-2">
			Category description
		</dt>
		<dd class="col-sm-10">
			@Html.DisplayFor(model => model.CategoryDescription)
		</dd>
		<dt class="col-sm-2">
			Category status
		</dt>
		<dd class="col-sm-10">
			@Html.DisplayFor(model => model.Status)
		</dd>
	</dl>
</div>
<div>
    <a asp-action="Edit" asp-route-seqId="@Model.SeqId">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
